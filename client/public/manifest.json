{
    "manifest_version": 3,
    "name": "React Playlist Fetcher",
    "description": "Fetch your online playlists for offline usage!",
    "version": "1.0.0",
    "author": "Srivathsav S",
    "action": {
        "default_title": "Playlist Fetcher",
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo.png",
            "128": "logo.png"
        }
    },
    "permissions": [
        "http://localhost:8000/"
    ]
}